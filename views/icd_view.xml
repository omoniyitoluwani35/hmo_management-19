<?xml version="1.0" encoding="utf-8"?>
<odoo>
	 <record id="view_icd_list" model="ir.ui.view">
        <field name="name">icd.list</field>
        <field name="model">icd</field>
		<field name="priority">2</field>
        <field name="arch" type="xml">
            <list string="ICD Classification">
                <field name="name"/>
                <field name="band"/>
            </list>
        </field>
    </record>

    <record id="view_icd_form" model="ir.ui.view">
        <field name="name">icd.form</field>
        <field name="model">icd</field>
        <field name="arch" type="xml">
		
            <form string="Claims diagnosis">
			<group>
                 <field name="name"/>
                <field name="band"/>
				<field name="active"/>
				</group>
            </form>
		
        </field>
    </record>
	
	<record id = "icd_list_view" model = "ir.actions.act_window">
        <field name = "name">ICD Classification</field>
        <field name = "type">ir.actions.act_window</field>
        <field name = "res_model">icd</field>
        <field name = "view_mode">list,form</field>
        <field name = "view_id" ref= "view_icd_list"/>
   </record>
   
</odoo>