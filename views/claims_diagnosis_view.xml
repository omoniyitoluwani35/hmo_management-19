<?xml version="1.0" encoding="utf-8"?>
<odoo>
	 <record id="view_claims_diagnosis_list" model="ir.ui.view">
        <field name="name">claims.diagnosis.list</field>
        <field name="model">claims.diagnosis</field>
		<field name="priority">2</field>
        <field name="arch" type="xml">
            <list string="Claims diagnosis">
                <field name="diagnosis_date"/>
                <field name="hcp_id"/>
				<field name="enrollee_id"/>
                <field name="claim_id"/>
				 <field name="icd_id"/>
				  <field name="details"/>
            </list>
        </field>
    </record>

    <record id="view_claims_diagnosis_form" model="ir.ui.view">
        <field name="name">claims.diagnosis.form</field>
        <field name="model">claims.diagnosis</field>
        <field name="arch" type="xml">
		
            <form string="Claims diagnosis">
			<group>
                 <field name="diagnosis_date"/>
                <field name="hcp_id"/>
				<field name="enrollee_id"/>
                <field name="claim_id"/>
				 <field name="icd_id"/>
				 <field name="details"/>
				</group>
            </form>
		
        </field>
    </record>
	
	<record id = "claims_diagnosis_list_view" model = "ir.actions.act_window">
        <field name = "name">Claim diagnosis</field>
        <field name = "type">ir.actions.act_window</field>
        <field name = "res_model">claims.diagnosis</field>
        <field name = "view_mode">list,form</field>
        <field name = "view_id" ref= "view_claims_diagnosis_list"/>
   </record>
   
    
   
   
</odoo>